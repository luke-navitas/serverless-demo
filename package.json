{
  "name": "slic-starter",
  "version": "1.0.0",
  "description": "A complete serverless starter application",
  "scripts": {
    "lint": "prettier-standard --lint --ignore-pattern \"*/scripts/**\" .",
    "format": "prettier-standard --format \"*/**/*.js\"",
    "coverage": "npm run test -- --cov --coverage-report=html",
    "test": "tap \"*/test/**/*.test.js\" --branches=100 --statements=100 --functions=100 --lines=100"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fourTheorem/slic-starter.git"
  },
  "keywords": [
    "serverless",
    "starter",
    "project",
    "aws"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fourTheorem/slic-starter/issues"
  },
  "homepage": "https://github.com/fourTheorem/slic-starter#readme",
  "devDependencies": {
    "aws-sdk-mock": "^4.3.0",
    "awscred": "^1.4.2",
    "husky": "^1.3.1",
    "prettier-standard": "^16.4.1",
    "proxyquire": "^2.1.3",
    "serverless": "^2.30.3",
    "tap": "^14.11.0"
  },
  "dependencies": {
    "@hapi/bourne": "^2.0.0",
    "aws-sdk": "^2.867.0",
    "aws-signed-axios": "^1.1.0",
    "aws-xray-sdk": "^3.2.0",
    "lambda-logger-middleware": "^1.0.3",
    "lodash": "^4.17.21",
    "middy": "^0.36.0",
    "middy-autoproxyresponse": "^0.1.0",
    "pino": "^6.11.2"
  }
}
